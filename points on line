#include<stdio.h>
struct line
{
int x1,y1;
int x2,y2;
int x,y;
};
struct lines
{
int n;
struct line num[100];
};
void input_function()
{ 
struct lines notice;
printf("Enter the number of outputs\n");
scanf("%d",&notice.n);
for(int i=0;i<notice.n;i++)
{

printf("Enter the values of x and y coordinate of point P1\n");
scanf("%d%d",&notice.num[i].x1,&notice.num[i].y1);
printf("Enter the values of x and y coordinate of point P2\n");
scanf("%d%d",&notice.num[i].x2,&notice.num[i].y2);
printf("Enter the values of x and y coordinate of point P\n");
scanf("%d%d",&notice.num[i].x,&notice.num[i].y);
}
}
int main()
{
input_function();
}


